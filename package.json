{
    "name": "@hytts/hytts",
    "version": "0.1.0",
    "author": "Axel Habermaier",
    "license": "MIT",
    "homepage": "https://github.com/HyTTS/HyTTS",
    "description": "HyTTS (pronounced 'heights') is a framework for server-side rendered web apps based on TypeScript. ",
    "keywords": [
        "TypeScript",
        "HyTTS",
        "server-side rendering",
        "JSX"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/HyTTS/HyTTS"
    },
    "main": "index.js",
    "exports": {
        "./jsx-runtime": "./index.js",
        "./jsx-dev-runtime": "./index.js",
        "./main.browser": "./main.browser.js",
        ".": "./index.js"
    },
    "scripts": {
        "build:browser": "tsup ./source/browser/main.browser.ts --dts --format=cjs --target=es2020 --env.NODE_ENV production && prettier --write ./dist/main.browser.d.ts",
        "build:server": "tsup ./source/index.ts --clean --dts --format=cjs --target=node18 --env.NODE_ENV production && echo 'export { JSX } from \"./index\";' > ./dist/jsx-runtime.d.ts && prettier --write ./dist/index.d.ts",
        "build:files": "cp ./package.json ./dist/package.json && cp ./README.md ./dist/README.md && cd ./dist && npm pkg delete scripts && npm pkg delete devDependencies",
        "build": "yarn build:server && yarn build:browser && yarn build:files",
        "publish:local": "yarn build && cd ./dist && npm pack --pack-destination ../tmp",
        "format:check": "prettier --check ./source",
        "format:fix": "prettier --write --loglevel warn ./source",
        "lint": "eslint ./ --max-warnings 0",
        "test:debug": "node --inspect=0.0.0.0:4106 node_modules/.bin/jest --runInBand --watch",
        "test:run": "yarn jest",
        "test:watch": "yarn test:run --watch",
        "test": "yarn test:watch",
        "typecheck:run": "tsc -b",
        "typecheck:watch": "yarn typecheck:run -w",
        "typecheck": "yarn typecheck:watch"
    },
    "dependencies": {
        "@js-joda/core": "5.5.3",
        "express": "4.18.2",
        "lodash": "4.17.21",
        "qs": "6.11.2",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@types/express": "4.17.17",
        "@types/jest": "29.5.1",
        "@types/lodash": "4.14.194",
        "@types/node": "18.16.1",
        "@typescript-eslint/eslint-plugin": "5.59.7",
        "@typescript-eslint/parser": "5.59.7",
        "esbuild": "0.17.19",
        "eslint-plugin-no-relative-import-paths": "1.5.2",
        "eslint": "8.41.0",
        "jest-circus": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "jest": "29.5.0",
        "prettier": "2.8.8",
        "tslib": "2.5.0",
        "tsup": "6.7.0",
        "typescript": "5.1.3"
    },
    "engines": {
        "node": ">=18"
    },
    "sideEffects": true
}